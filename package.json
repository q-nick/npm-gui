{
  "name": "npm-gui",
  "version": "2.0.1",
  "description": "Graphical tool for managing javascript project dependencies - in a friendly way.",
  "keywords": [
    "gui",
    "npm",
    "yarn",
    "bower",
    "view",
    "client",
    "dependencies",
    "packages",
    "installer",
    "console"
  ],
  "main": "index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepack": "npm run build:server && npm run build:client",
    "dev:server:webpack": "webpack --config webpack.server.config.js --watch --mode=development",
    "dev:server:nodemon": "nodemon --inspect --watch dist index.js localhost:9002",
    "dev:server": "concurrently \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\" ",
    "dev:client": "webpack-dev-server --config webpack.client.config.js --mode=development",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" ",
    "build:server": "webpack --config webpack.server.config.js --mode=production",
    "build:client": "webpack --config webpack.client.config.js --mode=production",
    "build": "npm run build:server && npm run build:client",
    "test:lint:css": "stylelint client",
    "test:lint:js": "tslint --project . ",
    "test:lint": "npm run test:lint:css && npm run test:lint:js",
    "test:unit": "env NODE_TEST=true TS_NODE_FILES=true mocha -r ts-node/register tests/*.test.ts tests/**/*.test.ts --timeout 30000",
    "test": "npm run test:unit && npm run test:lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q-nick/npm-gui.git"
  },
  "author": "PaweÅ‚ StefaÅ„ski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/q-nick/npm-gui/issues"
  },
  "bin": {
    "npm-gui": "./index.js"
  },
  "homepage": "https://github.com/q-nick/npm-gui#readme",
  "devDependencies": {
    "@types/analytics-node": "^3.1.1",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cross-spawn": "^6.0.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.7",
    "@types/ws": "^6.0.1",
    "analytics-node": "^3.3.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.1",
    "debounce": "^1.2.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "open-iconic": "^1.1.1",
    "opn": "^5.4.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "supertest": "^3.4.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.2.0"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {}
}
